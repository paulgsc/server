  "panels": [
    {
      "datasource": "Prometheus",
      "description": "Detects classic I/O-bound hangs: High load average + Low CPU usage",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "green",
                  "text": "System Healthy"
                },
                "1": {
                  "color": "red",
                  "text": "üö® I/O HANG DETECTED!"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 0.5
              }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "textMode": "name",
        "textSize": {
          "title": 16,
          "value": 14
        }
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "(\n  (node_load1 / on() (count(count(node_cpu_seconds_total) by (cpu)))) > 3\n)\nand\n(\n  (100 - avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100) < 30\n)\n",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "üö® SYSTEM HANG DETECTOR",
      "type": "stat"
    },
    {
      "datasource": "Prometheus",
      "description": "file-host receiving high request rate without 429 responses",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "green",
                  "text": "Rate Limiting Active"
