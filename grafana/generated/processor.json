        "orientation": "auto",
        "textMode": "name"
      },
      "targets": [
        {
          "datasource": "Prometheus",
          "expr": "(\n  sum(rate(http_requests_total[1m])) > 20\n)\nand\n(\n  sum(rate(http_requests_total{status=\"429\"}[1m])) == 0\n)\n",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "üö® High Load + No Rate Limiting",
      "type": "stat"
    },
